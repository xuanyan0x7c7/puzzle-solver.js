{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "ESNext",
    "sourceMap": true,
    "incremental": true,
    "outDir": "dist",
    "baseUrl": "src"
  }
}
